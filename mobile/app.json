{
    "expo": {
        "name": "Trainichi",
        "slug": "workout-planner",
        "version": "1.1.0",
        "orientation": "portrait",
        "icon": "./assets/images/catt.png",
        "scheme": "workout-planner",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/images/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#FAF7F2"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.linnaw626.workoutplanner",
            "infoPlist": {
                "ITSAppUsesNonExemptEncryption": false
            },
            "buildNumber": "1"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/catt.png",
                "backgroundColor": "#2A2A2A"
            },
            "edgeToEdgeEnabled": true,
            "package": "com.linnaw626.workoutplanner"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": ["expo-router", "expo-web-browser"],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "e160482b-86d6-4fba-9be4-e6a51fa5bd80"
            }
        }
    }
}
