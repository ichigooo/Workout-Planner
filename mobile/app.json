{
  "expo": {
    "name": "Trainichi",
    "slug": "workout-planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/catt.png",
    "scheme": "workout-planner",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7F7F7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.linnaw626.workoutplanner",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/catt.png",
        "backgroundColor": "#FF6B35"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.linnaw626.workoutplanner"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e160482b-86d6-4fba-9be4-e6a51fa5bd80"
      }
    }
  }
}
